{
  "public": true,
  "installCommand": "nvm install 14.18.1 && nvm use 14.18.1 && yarn install",
  "rewrites": [
    {
      "source": "/arts-gala",
      "destination": "https://arts-gala.hackclub.dev/"
    },
    {
      "source": "/arts-gala/_next/:path*",
      "destination": "https://arts-gala.hackclub.dev/_next/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    }
  ]
}
